#!/usr/bin/env bash

[[ -z $1 ]] && echo "gwmtest <WID>" && exit
WID=${1}
PSTN=add
DEFSTACK="$(gwmget SBARSTACK)default"
eval "$(wattr wh ${WID} | awk '{print  "W=" $1  "\nH=" $2}')"
eval "$(cat ${DEFSTACK} | awk '{print "SW=" $1 "\nSH=" $2}')"

((W>SW)) && PSTN=main
((H>SH)) && PSTN=main

echo $PSTN
