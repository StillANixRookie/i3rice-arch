{
   "app": {
      "background": {
         "scripts": [ "background.js" ]
      }
   },
   "description": "A simple and clean Material Design markdown editor with side by side preview",
   "file_handlers": {
      "text": {
         "extensions": [ "md", "txt", "readme" ],
         "types": [ "text/*" ]
      }
   },
   "icons": {
      "128": "assets/icon_128.png",
      "16": "assets/icon_16.png",
      "256": "assets/icon_256.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "41",
   "name": "Material Markdown",
   "offline_enabled": true,
   "permissions": [ "storage", "clipboardWrite", "contextMenus", "webview", "system.network", {
      "fileSystem": [ "write", "retainEntries", "directory" ]
   }, "\u003Call_urls>" ],
   "short_name": "Material Markdown",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.4.0"
}
